{
  "name": "coder-community",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "node scripts/clean",
    "reseed-mongodb": "nest build && node dist/reseed-mongodb.js",
    "nestgen": "nest generate",
    "gen": "nest build && node scripts/generate-api",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:ci": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch --runInBand --detectOpenHandles",
    "test:cov": "jest --coverage --runInBand --detectOpenHandles",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typedoc": "typedoc --out typedoc src",
    "doc": "compodoc src -p tsconfig.json -d docs -n Backend --hideGenerator",
    "doc:cn": "compodoc src -p tsconfig.json -d docs-cn -n Backend --hideGenerator --language zh-CN"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/swagger": "^4.5.10",
    "@typegoose/typegoose": "^7.2.0",
    "@types/chalk": "^2.2.0",
    "@types/uuid": "^8.0.0",
    "axios": "^0.19.2",
    "chalk": "^4.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "googleapis": "^52.1.0",
    "lodash": "^4.17.19",
    "mongoose": "^5.9.20",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "url-slug": "^2.3.1",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.2.0",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.6",
    "@types/jest": "25.2.3",
    "@types/lodash": "^4.14.157",
    "@types/mongoose": "^5.7.29",
    "@types/multer": "^1.4.3",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "del": "^5.1.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "extract-zip": "^2.0.1",
    "jest": "26.0.1",
    "line-replace": "^2.0.1",
    "mongodb": "^3.5.9",
    "prettier": "^1.19.1",
    "replace-in-file": "^6.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.17.7",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
